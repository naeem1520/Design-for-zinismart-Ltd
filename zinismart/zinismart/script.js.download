 $(document).ready(function() {
  $('.owl-carousel').owlCarousel({
    loop: true,
    autoplay: true,
    margin: 10,
    responsiveClass: true,
    responsive: {
      0: {
        items: 4,
        nav: true
      },
      600: {
        items: 4,
        nav: false
      },
      1000: {
        items: 7,
        nav: true,
        loop: true,
        margin: 20
      }
    }
  });

